<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.coolcode.bwamov">
    
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission-sdk-23 android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.noActionBar">
        <activity android:name=".DetailActivity"></activity>
        <activity android:name=".HomeActivity" />
        <activity android:name=".onboarding.OnboardingOneActivity" />
        <activity android:name=".onboarding.OnboardingTwoActivity" />
        <activity android:name=".onboarding.OnboardingThreeActivity" />
        <activity android:name=".sign.SignIn.SignInActivity" />
        <activity android:name=".sign.Signup.SignUpActivity" />
        <activity android:name=".sign.SignUpPhotoscreenActivity" />
        <activity android:name=".SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>